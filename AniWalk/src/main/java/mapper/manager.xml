<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="kr.pandorabox.aniwalk.manager">
	<select id="idcheck" parameterType="String" resultType="int">
		select count(*) from ani_manage where manager_id = #{manager_id}
	</select>
	
	<select id="pwcheck" parameterType="String" resultType="int">
		select count(*) from ani_manage where manage_pw = #{manage_pw}
	</select>
</mapper>